{
    "name": "spomky-labs/oauth2-server-library",
    "type": "library",
    "description": "OAuth2 Server Library",
    "license": "MIT",
    "keywords": ["oauth2", "server", "library"],
    "homepage": "https://github.com/Spomky-Labs/oauth2-server-library",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky-Labs/oauth2-server-library/contributors"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zendframework/zend-diactoros": "~1.1.0",
        "spomky-labs/defuse-generator": "~1.0.0",
        "spomky-labs/jose-interface": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "symfony/psr-http-message-bridge": "~0.2",
        "satooshi/php-coveralls": "dev-master",
        "spomky-labs/jose-service": "~0.1.0"
    },
    "autoload": {
        "psr-4": {
            "OAuth2\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAuth2\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
